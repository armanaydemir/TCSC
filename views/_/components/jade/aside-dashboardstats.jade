html
  body
    .container.col-lg-4.col-md-4.col-sm-4(style='margin-top: 5em;')
      .panel.panel-default
        .panel-heading
          h3.panel-title
            | TEAM NAME HERE
            span(style='float:right;padding-right:0.5em;')
              img.img-rounded(src='images/useridenticon.png', width='20px', height='20px')
        .panel-body
          p(style='line-height:0.1em')
            | Points
            span(style='float:right; color:dimgrey; padding-right:1em;') 0
          hr
          p(style='line-height:0.1em')
            | Questions Answered
            span(style='float:right; color:dimgrey; padding-right:1em;') 0
          hr
          p(style='line-height:0.1em')
            | Other
            span(style='float:right; color:dimgrey; padding-right:1em;') 0
      .panel.panel-default
        .panel-heading
          h3.panel-title  Team Name Chat 
        .panel-body
          ul#messages
            form(action='')
              input#m
              button(type='button', autocomplete='off') Send
<script src="https://cdn.socket.io/socket.io-1.2.0.js"></script>
<script src="http://code.jquery.com/jquery-1.11.1.js"></script>
script.
  var socket = io();
  $(function(){
    $('button').click(function(){
      console.log("comeonnnnn");
      socket.emit('send_message', $('#m').val());
      $('#messages').append($('<li>').text($('#m').val()));
      $('#m').val('');
      return false;
    });
  });
  
  socket.on('new_message' + #{config.user.team}, function(msg, usr){
    if(usr != #{config.user.id}){$('#messages').append($('<li>').text(msg));}
  });